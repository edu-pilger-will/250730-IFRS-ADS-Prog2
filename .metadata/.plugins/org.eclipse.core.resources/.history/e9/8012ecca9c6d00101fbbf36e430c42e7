package exemplos;

public class Dados {

	public static void main(String[] args) {

		// Tipos de Dados Primitivos
		
		// Byte (8bits)
		byte var1 = 127;
		System.out.println(var1);
		var1 += 1;
		System.out.println(var1);

		short var2 = 1000;
		System.out.println(var2);
		
		int var3 = 123456;
		System.out.println(var3);
		
		long var4 = 1234567890;
		System.out.println(var4);
		
//		float var5 = 5.98654;
//		System.out.println(var5);
		
	}

}
